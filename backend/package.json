    "pg": "^8.3.3",
    "sequelize": "^6.3.5",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-dist": "^3.33.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/express": "^4.17.8",
    "@types/express-validator": "^3.0.0",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mocha": "^8.0.3",
    "@types/sequelize": "^4.28.9",
